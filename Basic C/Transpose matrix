#include<stdio.h>
int main(){
    int m[4][4];
    int t[4][4];

    printf("Enter elements of first matrix : ");
    for(int i=0;i<4;i++){
        for(int j=0;j<4;j++){
            scanf("%d",&m[i][j]);
        }
    }
    printf("\nMatrix 1\n");
     for(int i=0;i<4;i++){
        for(int j=0;j<4;j++){
            printf("%d ",m[i][j]);
        }
        printf("\n");
    }

    for(int i=0;i<4;i++){
        for(int j=0;j<4;j++){
            t[j][i] = m[i][j];
        }
        printf("\n");
    }
    printf("Transposed matrix\n");
     for(int i=0;i<4;i++){
        for(int j=0;j<4;j++){
            printf("%d ",t[i][j]);
        }
        printf("\n");
     }
}
